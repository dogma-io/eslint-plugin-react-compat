{
  "name": "eslint-plugin-react-compat",
  "version": "0.1.1",
  "description": "Make sure your React code is compatible with Inferno and Preact.",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "nodely -s ./src -o ./lib",
    "postpublish": "codecov",
    "prepublishOnly": "npm run build",
    "test": "lintly app src && flow && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dogma-io/eslint-plugin-react-compat.git"
  },
  "keywords": [
    "array",
    "check",
    "code",
    "component",
    "eslint",
    "inferno",
    "javascript",
    "js",
    "plugin",
    "preact",
    "quality",
    "react",
    "render",
    "return",
    "rules"
  ],
  "author": "Matthew Dahl <https://github.com/dogma-io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dogma-io/eslint-plugin-react-compat/issues"
  },
  "homepage": "https://github.com/dogma-io/eslint-plugin-react-compat#readme",
  "devDependencies": {
    "babel-core": "^7.0.0-0",
    "babel-jest": "22.4.3",
    "codecov": "3.0.2",
    "jest": "22.4.3",
    "lintly": "0.1.3",
    "nodely": "0.1.5"
  },
  "engines": {
    "node": ">= 6"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageReporters": [
      "json-summary",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "testRegex": "/__tests__/.*-test\\.js$"
  }
}
